  "timepicker": {},
  "schemaVersion": 38,
  "version": 3,
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "label": "Datasource",
        "pluginId": "prometheus",
        "query": "prometheus",
        "current": {},
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "refresh": 1
      }
    ]
